#include<iostream.h>
#include<conio.h>
#include<process.h>
struct node
{
  int item;
  node*next;
}*front,*rear,*newptr,*save,*ptr;
node* create_node(int n)
{
ptr=new node;
ptr->item=n;
ptr->next=NULL;
return ptr;
}
void insert(node*np)
{
if(front==NULL)
front=rear=np;
else
{
rear->next=save;
rear=np;
}
}
void del()
{
if(front==NULL)
  cout<<"UNDERFLOW!!\n";
  else
   {
     ptr=front;
     front=front->next;
     delete ptr;
   }
}
void display(node*np)
{
   while(np!=NULL)
   {
    cout<<np->item<<"->";
    np=np->next;
   }
  cout<<"\n";
}

